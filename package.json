  "version": "0.9.1",
  "private": true,
  "engines": {
    "node": ">= 10.6.x"
  },
  "scripts": {
    "start": "node start.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*' BROWSER=none\" \"npm run client\"",
