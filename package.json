    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "es6-promisify": "^6.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.22.1",
    "html-to-text": "^5.1.1",
    "jimp": "^0.13.0",
    "juice": "^8.0.0",
    "moment": "^2.29.4",
    "mongoose": "^5.9.18",
    "mongoose-mongodb-errors": "0.0.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.8",
